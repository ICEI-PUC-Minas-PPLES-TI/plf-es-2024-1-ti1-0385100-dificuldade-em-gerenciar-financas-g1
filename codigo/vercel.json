{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/vercel.js": {
      "memory": 1024,
      "includeFiles": "public/assets/data/db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/vercel.js"
    }
  ]
}
